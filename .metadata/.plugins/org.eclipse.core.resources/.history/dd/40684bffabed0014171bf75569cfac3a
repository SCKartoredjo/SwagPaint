import java.util.Scanner;

public class MyText {

	String text = " ";
	int x = 0;
	int y = 0;
	Scanner cin = new Scanner(System.in);
	
	public MyText(int x, int y, String text){
		this.x = x;
		this.y = y ;
		changeText (text);
	}
	
	public void changeText (String text){
		//text = cin.nextLine();
		System.out.println("Text issss: " + text + "x= " + x + "y=" + y);
			
	}
	
}
